{
  "version": 2,
  "framework": null,
  "installCommand": "npm install",
  "buildCommand": "npm run build",
  "outputDirectory": ".",
  "functions": {
    "api/**/*.ts": {
      "maxDuration": 60,
      "includeFiles": "dist/**"
    }
  },
  "rewrites": [
    { "source": "/", "destination": "/api/index" }
  ]
}
